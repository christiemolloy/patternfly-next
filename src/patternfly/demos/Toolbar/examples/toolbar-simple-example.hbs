{{#> toolbar pf-l-toolbar--modifier="pf-u-display-flex pf-u-justify-content-space-between"}}
  {{#> toolbar-section aria-label="First section"}}
    {{#> toolbar-group}}
      {{#> toolbar-item pf-l-toolbar__item--modifier="pf-u-mx-md"}}
        {{#> input-group}}
          {{#> dropdown id="dropdown-example-collapsed"}}
            All
            <i class="fas fa-caret-down pf-c-dropdown__toggle-icon"></i> 
          {{/dropdown}}
          {{#> input-group-action input-group-action--modifier="pf-m-align-right" inputGroupTextType="button" aria-label="popover for input"}}
            <i class="fas fa-search aria-hidden="true"></i>
          {{/input-group-action}}
          {{#> form-controls controlType="input" input="true" type="text" id="textInput10" name="textInput10" placeholder="Filter..." aria-label="standard input example" }}
          {{/form-controls}}
        {{/input-group}}
      {{/toolbar-item}}
    {{/toolbar-group}}

    {{#> toolbar-group}}
      {{#> toolbar-item pf-l-toolbar__item--modifier="pf-u-mx-md"}}
        {{#> dropdown id="dropdown-example-collapsed" pf-c-dropdown--HasToggleIcon="true"}}
          All
        {{/dropdown}}
      {{/toolbar-item}}
      {{#> toolbar-item}}
        <i class="fas fa-sort-alpha-down"></i>
      {{/toolbar-item}}
    {{/toolbar-group}}
    {{#> toolbar-group pf-l-toolbar__group--modifier="pf-u-mx-md"}}
      {{#> toolbar-item pf-l-toolbar__item--modifier="pf-u-mx-sm"}}
        <i class="fas fa-list"></i>
      {{/toolbar-item}}
        {{#> toolbar-item pf-l-toolbar__item--modifier="pf-u-mx-sm"}}
          <i class="fas fa-table"></i>
        {{/toolbar-item}}
        {{#> toolbar-item pf-l-toolbar__item--modifier="pf-u-mx-sm"}}
          Action
        {{/toolbar-item}}
        {{#> toolbar-item pf-l-toolbar__item--modifier="pf-u-mx-sm"}}
          {{#> button btnClass="pf-m-primary"}}
            Action
          {{/button}}
        {{/toolbar-item}}
        {{#> toolbar-item pf-l-toolbar__item--modifier="pf-u-mx-sm"}}
          {{#> dropdown id="dropdown-example-kebab" pf-c-dropdown--Modifier="pf-m-action pf-m-align-right"}}
            <i class="fas fa-ellipsis-v"></i>
          {{/dropdown}}
        {{/toolbar-item}}
    {{/toolbar-group}}
  {{/toolbar-section}} 
  {{#> toolbar-section aria-label="Second section"}}
    {{#> toolbar-group pf-l-toolbar__group--modifier="pf-u-mx-md"}}
      {{#> toolbar-item}}
        {{#> check pf-c-check-type="checkbox" name="unselected checkbox example" aria-label="unselected checkbox example"}}
        {{/check}}
        <i class="fas fa-caret-down pf-c-dropdown__toggle-icon"></i> 
        Repository
      {{/toolbar-item}}
    {{/toolbar-group}}
    {{#> toolbar-group pf-l-toolbar__group--modifier="pf-u-mr-xl"}}
      {{#> toolbar-item pf-l-toolbar__item--modifier="pf-u-mx-lg"}}
        Branches
      {{/toolbar-item}}
      {{#> toolbar-item pf-l-toolbar__item--modifier="pf-u-mx-lg"}}
        Pull Requests
      {{/toolbar-item}}
      {{#> toolbar-item pf-l-toolbar__item--modifier="pf-u-mx-lg"}}
        Work Spaces
      {{/toolbar-item}}
      {{#> toolbar-item pf-l-toolbar__item--modifier="pf-u-mx-lg"}}
        Last Commit
      {{/toolbar-item}}
    {{/toolbar-group}}
  {{/toolbar-section}}
{{/toolbar}}
