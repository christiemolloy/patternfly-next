{{#> alert pf-c-alert--Modifier="pf-m-warning" pf-c-alert--AriaLabel="Warning Notification"}}
  {{#> alert-icon pf-c-alert-icon--HasExclamationTriangleIcon="true"}}
  {{/alert-icon}}
  {{#> alert-body}}
    {{#> alert-title}}
      {{#> sr sr--modifier="pf-u-sr-only"}}
        Warning:
      {{/sr}}
      Warning notification title
    {{/alert-title}}
    This is a description of the notification content.
  {{/alert-body}}
{{/alert}}
