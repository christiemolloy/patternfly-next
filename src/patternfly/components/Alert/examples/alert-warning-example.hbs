{{#> alert alert--modifier="pf-m-warning" alert--attribute='aria-label="Warning Notification"'}}
  {{#> alert-icon alert-icon--warning="true"}}
  {{/alert-icon}}
  {{#> alert-body}}
    {{#> alert-title}}
      {{#> accessibility accessibility--type="sr-only"}}Warning: {{/accessibility}}
      Warning notification title
    {{/alert-title}}
    {{#> alert-description}}
			Warning notification description.
		{{/alert-description}}
    {{#> alert-link alert-link--modifier="pf-c-button pf-m-link"}}
			This is a link.
		{{/alert-link}}
  {{/alert-body}}
{{/alert}}
