{{#> alert pf-c-alert--modifier="pf-m-warning" pf-c-alert--attribute='aria-label="Warning Notification"'}}
  {{#> alert-icon pf-c-alert-icon--warning="true"}}
  {{/alert-icon}}
  {{#> alert-body}}
    {{#> alert-title}}
      {{#> sr sr--modifier="pf-u-sr-only"}}
        Warning:
      {{/sr}}
      Warning notification title
    {{/alert-title}}
    <p>This is a description of the notification content.</p>
  {{/alert-body}}
{{/alert}}
