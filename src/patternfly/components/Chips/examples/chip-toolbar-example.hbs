{{#> toolbar}}
  {{#> toolbar-section toolbar-section--modifier="pf-u-justify-content-space-between pf-u-mx-xl pf-u-my-md"}}
    {{#> toolbar-group}}
      {{#> toolbar-item toolbar-item--modifier="pf-u-mr-2xl"}}
        {{#> input-group }}
          {{#> dropdown id="dropdown-example-collapsed" dropdown--HasToggleIcon="true"}}
            All
          {{/dropdown}}
          {{#> form-control controlType="input" form-control--attribute='input="true" type="text" id="textInput10" name="textInput10"  placeholder="Filter..." aria-label="filter input with popup button"'}}
          {{/form-control}}
          {{#> button button--modifier="pf-m-tertiary" button--HasSearchIcon="true" button--attribute='id="textAreaButton2"'}}
          {{/button}}
        {{/input-group}}
      {{/toolbar-item}}
      {{#> toolbar-item toolbar-item--modifier="pf-u-mr-lg"}}
        {{#> dropdown id="dropdown-example-collapsed" dropdown--HasToggleIcon="true"}}
          All
        {{/dropdown}}
      {{/toolbar-item}}
      {{#> toolbar-item}}
        {{#> button button--modifier="pf-c-button pf-m-plain" button--HasSortAlphaDownIcon="true" button--attribute='aria-label="Sort A-Z"'}}
        {{/button}}
    {{/toolbar-item}}
    {{/toolbar-group}}
    {{#> toolbar-group}}
      {{#> toolbar-item toolbar-item--modifier="pf-u-mr-md"}}
        {{#> button button--modifier="pf-m-plain" button--HasTableIcon="true" button--attribute='aria-label="Table"'}}
        {{/button}}
      {{/toolbar-item}}
      {{#> toolbar-item}}
        {{#> button button--modifier="pf-m-plain" button--HasListIcon="true" button--attribute='aria-label="List"'}}
        {{/button}}
      {{/toolbar-item}}
      {{#> toolbar-item toolbar-item--modifier="pf-u-mx-md"}}
        {{#> button button--modifier="pf-m-link"}}
          Action
        {{/button}}
      {{/toolbar-item}}
      {{#> toolbar-item toolbar-item--modifier="pf-u-mr-md"}}
        {{#> button button--modifier="pf-m-primary"}}
          Action
        {{/button}}
      {{/toolbar-item}}
      {{#> toolbar-item}}
        {{#> dropdown dropdown--HasKebabIcon="true" dropdown--modifier="pf-m-plain"}}
        {{/dropdown}}
      {{/toolbar-item}}
    {{/toolbar-group}}
  {{/toolbar-section}}
  {{#> toolbar-section toolbar-section--modifier="pf-u-justify-content-space-between pf-u-mx-xl pf-u-my-md"}}
    {{#> toolbar-group}}
      {{#> toolbar-item toolbar-item--modifier="pf-u-mr-md"}}
        17 of 80 items
      {{/toolbar-item}}
      {{#> toolbar-item}}
        {{#> chip-group chip-group--attribute='role="group of chips"'}}
          {{#> chip-label}}
            United States
          {{/chip-label}}
          {{#> chip}}
            {{#> chip-text}}
              Texas
            {{/chip-text}}
            {{#> button button--HasTimesCircleIcon="true" button--attribute='aria-label="Remove"'}}
            {{/button}}
          {{/chip}}
          {{#> chip}}
            {{#> chip-text}}
              Washington D.C
            {{/chip-text}}
            {{#> button button--HasTimesCircleIcon="true" button--attribute='aria-label="Remove"'}}
            {{/button}}
          {{/chip}}
          {{#> chip}}
            {{#> chip-text}}
              Massachusetts
            {{/chip-text}}
            {{#> button button--HasTimesCircleIcon="true" button--attribute='aria-label="Remove"'}}
            {{/button}}
          {{/chip}}
        {{/chip-group}}
      {{/toolbar-item}}
      {{#> toolbar-item toolbar-item--modifier="pf-u-ml-sm"}}
        {{#> chip chip--modifier="pf-m-overflow"}}
          {{#> button}}
            {{#> chip-text}}
              4 more
            {{/chip-text}}
          {{/button}}
        {{/chip}}
      {{/toolbar-item}}
    {{/toolbar-group}}
  {{/toolbar-section}}
{{/toolbar}}
